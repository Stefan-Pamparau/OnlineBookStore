<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <import resource="classpath:spring/jpa/database-spring.xml"/>

    <alias name="defaultAuthorService" alias="authorService"/>
    <bean id="defaultAuthorService" class="com.iquestgroup.service.impl.jpa.DefaultAuthorService"/>

    <alias name="defaultBookService" alias="bookService"/>
    <bean id="defaultBookService" class="com.iquestgroup.service.impl.jpa.DefaultBookService"/>

    <alias name="defaultClientService" alias="clientService"/>
    <bean id="defaultClientService" class="com.iquestgroup.service.impl.jpa.DefaultClientService"/>

</beans>